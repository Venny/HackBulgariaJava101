package week08;

public class Main {

	public static class MyThread extends Thread {
		
		public void start() {
			System.out.println("Why Hellow There!");
		}
	}
	
	public static void main(String[] args) {
		MyThread thread = new MyThread();
		thread.start();
		System.out.println("Hellow to you too!");
	}
}

package week01;

public class Task09 {
public static long getLargestPalindrome(long n) {
	boolean isPalindrome = false;
	String number = Long.toString(n);
	char[] array = number.toCharArray();
	for (int i = 0; i < array.length; i++) {
		if (array[i] == array[array.length - (i + 1)]) {
			isPalindrome = true;
		}
		else
			isPalindrome = false;
	}
}
}
